<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.msi_gl62.co_work_android_uset">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/facebook_app_id" />
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.login.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.register.RegisterActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.home.HomeFragment"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.forgot.ForgotActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.forgot.ForgotActivityFinish"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.detail.DetailPopularActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.detail.DetailNearbyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.profile.ProfileUserFragment"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.msigl62.coworkandroiduset.ui.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.msigl62.coworkandroiduset.ui.detail.GalleryActivity" />
        <activity android:name="com.example.msigl62.coworkandroiduset.ui.reserve.ReserveActivity" />
        <activity android:name="com.example.msigl62.coworkandroiduset.ui.profile.EditProfileUserActivity" />
        <activity android:name="com.example.msigl62.coworkandroiduset.ui.reserve.ReserveSummaryActivity" />
        <activity android:name="com.example.msigl62.coworkandroiduset.ui.show.ShowAllNearbyActivity" />
        <activity android:name="com.example.msigl62.coworkandroiduset.ui.reserve.ReserveOTPActivity"></activity>
    </application>

</manifest>